from django.contrib import admin
from models import Challenge, ChallengeEntry, ValidEntry, UserProfile, League

class ChallengeAdmin(admin.ModelAdmin):
    pass
admin.site.register(Challenge, ChallengeAdmin)

class ChallengeEntryAdmin(admin.ModelAdmin):
    pass
admin.site.register(ChallengeEntry, ChallengeEntryAdmin)

class ValidEntryAdmin(admin.ModelAdmin):
    pass
admin.site.register(ValidEntry, ValidEntryAdmin)

class UserProfileAdmin(admin.ModelAdmin):
    pass
admin.site.register(UserProfile, UserProfileAdmin)

class LeagueAdmin(admin.ModelAdmin):
    pass
admin.site.register(League, LeagueAdmin)
